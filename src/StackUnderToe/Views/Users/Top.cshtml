@model IEnumerable<StackUnderToe.User>
@{
    ViewBag.Title = "Stack Under Toe";
}

<h2>Top Users</h2>
<p>Users that have the most reputation.</p>

<ul>
    @foreach (var user in Model)
    {
        <li>@user.Reputation - @Html.ActionLink(user.DisplayName, "Details", new { controller = "Users", id = user.Id }) </li>
    }
</ul>
